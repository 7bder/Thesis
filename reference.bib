	author = {Larsen, Jacob and El-Geneidy, Ahmed},
	file = {:D$\backslash$:/OneDrive - 九州大学/参考论文/読んでる論文/20171013/Beyond the quarter mile- re-examining travel distances by active transportation.pdf:pdf},
	issn = {11883774},
	journal = {Canadian Journal of Urban Research},
	keywords = {Cycling distance,Distance decay,Walking distance},
	number = {1 SUPPL.},
	pages = {70--88},
	title = {{Beyond the quarter mile: Re-examining travel distances by active transportation}},
	volume = {19},
	year = {2010}
}
