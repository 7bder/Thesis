	file = {:C$\backslash$:/Users/ChenQi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Krygsman, Dijst, Arentze - 2004 - Multimodal public transport an analysis of travel time elements and the interconnectivity ratio.pdf:pdf},
	isbn = {0967-070X},
	issn = {0967070X},
	journal = {Transport Policy},
	keywords = {Access,Egress,Multimodal public transport trip},
