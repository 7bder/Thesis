	author = {{Weinstein Agrawal}, Asha and Schlossberg, Marc and Irvin, Katja},
	doi = {10.1080/13574800701804074},
	file = {:D$\backslash$:/OneDrive - 九州大学/参考论文/読んでる論文/20170919/How far by which route and why A spatial analysis of pedestrian preference.pdf:pdf},
	isbn = {1357-4809},
	issn = {1357-4809},
	journal = {Journal of Urban Design},
	number = {1},
	pages = {81--98},
	title = {{How Far, by Which Route and Why? A Spatial Analysis of Pedestrian Preference}},
